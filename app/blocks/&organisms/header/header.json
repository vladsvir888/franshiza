      }, {
        "text": {
          "text": "Контакты"
        }
      }, {
        "text": {
          "text": "Войти"
        }
      }, {
        "text": {
          "text": "Зарегистрироваться"
        }
      }]
    },
    "tel_block": {
      "text": "Ваш каталог франшиз №1",
      "link": {
        "href": "tel:+74951855663",
        "text": {
          "text": "+7 (495) 185-56-63"
        },
        "icon": {
          "class": "header__tel-icon",
          "name": "sprite-mono.svg#phone"
        }
      }
    },
    "main_nav": {
      "class": "header__main-nav",
      "attrs": "aria-label='Главное'",
