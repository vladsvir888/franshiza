      },
      "icon": {
        "name": "sprite-mono.svg#four_squares"
      }
    },
    "search": {
      "input": {
        "wrap": {
          "class": "search__input-block"
        },
        "element": {
          "class": "search__input",
          "type": "search",
          "text": "Поиск, например, магазин",
          "name": "search"
        },
        "label": {
          "btn": {
            "class": "search__btn",
