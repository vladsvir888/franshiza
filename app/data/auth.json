{
  "meta": {
    "title": "Авторизация",
    "description": "Авторизация"
  },
  "auth": {
    "breadcrumbs": {
      "items": [{
        "text": {
          "text": "Главная"
        }
      }, {
        "text": {
          "text": "Авторизация"
        }
      }]
    },
    "title": {
      "text": "Авторизация"
    },
    "form_block": {
      "title": {
        "level": 2,
        "text": "Войти через"
      },
      "socials": {
        "list": {
          "items": [{
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#gmail"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#vk"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#yandex"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#mail"
            }
          }]
        }
      },
      "inputs": [{
        "element": {
          "type": "email",
          "text": "Введите E-mail",
          "name": "email",
          "attrs": "required data-pristine-required-message='Это обязательное поле' data-pristine-email-message='Введите правильный E-mail. Например,&nbsp;myemail@gmail.com'"
        }
      }, {
        "element": {
          "type": "password",
          "text": "Введите пароль",
          "name": "password",
          "attrs": "required data-pristine-required-message='Это обязательное поле'"
        }
      }],
      "button_forgot_password": {
        "href": "#",
        "text": {
          "text": "Забыли пароль?"
        }
      },
      "button": {
        "text": {
          "text": "Войти"
        },
        "attrs": "type='submit'"
      },
      "button_registration": {
        "href": "#",
        "text": {
          "text": "Зарегистрироваться"
        }
      }
    }
  }
}