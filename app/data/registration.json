{
  "meta": {
    "title": "Регистрация",
    "description": "Регистрация"
  },
  "registration": {
    "breadcrumbs": {
      "items": [{
        "text": {
          "text": "Главная"
        }
      }, {
        "text": {
          "text": "Регистрация"
        }
      }]
    },
    "title": {
      "text": "Бесплатная регистрация"
    },
    "form_block": {
      "title": {
        "level": 2,
        "text": "Войти через"
      },
      "socials": {
        "list": {
          "items": [{
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#gmail"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#vk"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#yandex"
            }
          }, {
            "icon": {
              "class": "socials__icon",
              "name": "sprite-multi.svg#mail"
            }
          }]
        }
      },
      "inputs": [{
        "element": {
          "type": "text",
          "text": "Введите имя",
          "name": "name",
          "attrs": "required data-pristine-required-message='Это обязательное поле'"
        }
      }, {
        "element": {
          "type": "email",
          "text": "Введите E-mail",
          "name": "email",
          "attrs": "required data-pristine-required-message='Это обязательное поле' data-pristine-email-message='Введите правильный E-mail. Например,&nbsp;myemail@gmail.com'"
        }
      }, {
        "element": {
          "type": "tel",
          "text": "Введите телефон",
          "name": "tel",
          "attrs": "required data-pristine-required-message='Это обязательное поле' data-pristine-phone-validator='Введите только числа (от 0 до 9)'"
        }
      }],
      "button": {
        "text": {
          "text": "Зарегистрироваться"
        },
        "attrs": "type='submit'"
      },
      "note": "Отправляя запрос, я <a class='btn btn--underline' href='#'>соглашаюсь на обработку</a> персональных данных"
    }
  }
}